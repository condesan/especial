INSERT INTO `esp_locales_source` VALUES ('1289',' modules/comment/comment.admin.inc:183','default','There do not appear to be any comments to delete or your selected comment was deleted by another administrator.','none'),('1290',' modules/comment/comment.admin.inc:188','default','Are you sure you want to delete these comments and all their children?','none'),('1291',' modules/comment/comment.module:1197','default','You have to specify a valid author.','none'),('1292',' modules/comment/comment.module:1208','default','The name you used belongs to a registered user.','none'),('1293',' modules/comment/comment.module:1213','default','You have to leave your name.','none'),('1294',' modules/comment/comment.module:1218','default','The e-mail address you specified is not valid.','none'),('1295',' modules/comment/comment.module:1222','default','You have to leave an e-mail address.','none'),('1296',' modules/comment/comment.module:1227','default','The URL of your homepage is not valid. Remember that it must be fully qualified, i.e. of the form <code>http://example.com/directory</code>.','none'),('1297',' modules/comment/comment.module:1532','default','(No subject)','none'),('1298',' modules/comment/comment.module:1647','default','Select your preferred way to display the comments and click \"Save settings\" to activate your changes.','none'),('1299',' modules/comment/comment.module:1648','default','Comment viewing options','none'),('1300',' modules/comment/comment.module:1801','default','<a href=\"@login\">Login</a> or <a href=\"@register\">register</a> to post comments','none'),('1301',' modules/comment/comment.admin.inc:286','default','Comment: deleted %subject.','none'),('1302',' modules/comment/comment.admin.inc:56','default','Posted in','none'),('1303',' modules/comment/comment.admin.inc:280','default','Cannot delete non-existent comment.','none'),('1304',' modules/comment/comment.module:130','default','Comment permissions are assigned to user roles, and are used to determine whether anonymous users (or other roles) are allowed to comment on posts. If anonymous users are allowed to comment, their individual contact information may be retained in cookies stored on their local computer for use in later comment submissions. When a comment has no replies, it may be (optionally) edited by its author. The comment module uses the same input formats and HTML tags available when creating other forms of content.','none');
INSERT INTO `esp_locales_source` VALUES ('1305',' modules/comment/comment.module:131','default','For more information, see the online handbook entry for <a href=\"@comment\">Comment module</a>.','none'),('1306',' modules/comment/comment.module:134','default','Below is a list of the latest comments posted to your site. Click on a subject to see the comment, the author\'s name to edit the author\'s user information, \'edit\' to modify the text, and \'delete\' to remove their submission.','none'),('1307',' modules/comment/comment.module:561','default','Forces a user to look at their comment by clicking on a \'Preview\' button before they can actually add the comment','6.19'),('1308',' modules/comment/comment.module:794,793','default','Comment: unauthorized comment submitted or comment submitted to a closed post %subject.','none'),('1309',' modules/comment/comment.module:1833','default','Submitted by !username on @datetime.','none'),('1310',' modules/comment/comment.module:129','default','The comment module allows visitors to comment on your posts, creating ad hoc discussion boards. Any <a href=\"@content-type\">content type</a> may have its <em>Default comment setting</em> set to <em>Read/Write</em> to allow comments, or <em>Disabled</em>, to prevent comments. Comment display settings and other controls may also be customized for each content type (some display settings are customizable by individual users).','none'),('1311',' modules/comment/comment.module:1075,1081,1088','default','Delete the selected comments','none'),('1312',' modules/dblog/dblog.admin.inc:91,119','default','No log messages available.','none'),('1313',' modules/dblog/dblog.admin.inc:150','default','Location','6.19'),('1314',' modules/dblog/dblog.admin.inc:162,230','default','Severity','none'),('1315',' modules/dblog/dblog.admin.inc:107','default','Count','none'),('1316',' modules/dblog/dblog.admin.inc:270','default','Filter log messages','none'),('1317',' modules/dblog/dblog.admin.inc:307','default','You must select something to filter by.','none'),('1318',' modules/dblog/dblog.admin.inc:18','default','Discard log entries above the following row limit','none');
INSERT INTO `esp_locales_source` VALUES ('1319',' modules/dblog/dblog.admin.inc:21','default','The maximum number of rows to keep in the database log. Older entries will be automatically discarded. (Requires a correctly configured <a href=\"@cron\">cron maintenance task</a>.)','none'),('1320',' modules/dblog/dblog.module:21','default','The dblog module monitors your system, capturing system events in a log to be reviewed by an authorized individual at a later time. This is useful for site administrators who want a quick overview of activities on their site. The logs also record the sequence of events, so it can be useful for debugging site errors.','none'),('1321',' modules/dblog/dblog.module:22','default','The dblog log is simply a list of recorded events containing usage data, performance data, errors, warnings and operational information. Administrators should check the dblog report on a regular basis to ensure their site is working properly.','none'),('1322',' modules/dblog/dblog.module:23','default','For more information, see the online handbook entry for <a href=\"@dblog\">Dblog module</a>.','none'),('1323',' modules/dblog/dblog.module:26','default','The dblog module monitors your website, capturing system events in a log to be reviewed by an authorized individual at a later time. The dblog log is simply a list of recorded events containing usage data, performance data, errors, warnings and operational information. It is vital to check the dblog report on a regular basis as it is often the only way to tell what is going on.','none'),('1324',' modules/dblog/dblog.module:47','default','Settings for logging to the Drupal database logs. This is the most common method for small to medium sites on shared hosting. The logs are viewable from the admin pages.','6.19'),('1325',' modules/dblog/dblog.module:55','default','Recent log entries','6.19'),('1326',' modules/dblog/dblog.module:56','default','View events that have recently been logged.','6.19'),('1327',' modules/dblog/dblog.module:63','default','Top \'page not found\' errors','6.19');
INSERT INTO `esp_locales_source` VALUES ('1328',' modules/dblog/dblog.module:64','default','View \'page not found\' errors (404s).','6.19'),('1329',' modules/dblog/dblog.module:71','default','Top \'access denied\' errors','6.19'),('1330',' modules/dblog/dblog.module:72','default','View \'access denied\' errors (403s).','6.19'),('1331',' modules/filter/filter.module:671','default','Filter HTML tags','none'),('1332',' modules/filter/filter.module:495','default','Input format','6.19'),('1333',' modules/filter/filter.module:209','default','Table','6.19'),('1334',' modules/filter/filter.admin.inc:130','default','Filters','6.19'),('1335',' modules/filter/filter.module:203','default','Preformatted','none'),('1336',' modules/filter/filter.module:211','default','Deleted','none'),('1337',' modules/filter/filter.module:252','default','Greater than','none'),('1338',' modules/filter/filter.module:253','default','Less than','none'),('1339',' modules/filter/filter.module:692','default','Spam link deterrent','none'),('1340',' modules/filter/filter.module:157','default','administer filters','none'),('1341',' modules/filter/filter.module:615','default','Line break converter','6.19'),('1342',' modules/filter/filter.module:199','default','Underlined','none'),('1343',' modules/filter/filter.module:71','default','Input formats','6.19'),('1344',' modules/filter/filter.module:221','default','Subtitle','none'),('1345',' modules/filter/filter.module:673','default','Escape all tags','none'),('1346',' modules/filter/filter.module:673','default','Strip disallowed tags','none'),('1347',' modules/filter/filter.pages.inc:35','default','input formats','none'),('1348',' modules/filter/filter.module:131','default','Rearrange','6.19'),('1349',' modules/filter/filter.module:694','default','If enabled, Drupal will add rel=\"nofollow\" to all links, as a measure to reduce the effectiveness of spam links. Note: this will also prevent valid links from being followed by search engines, therefore it is likely most effective when enabled for anonymous users.','none'),('1350',' modules/filter/filter.module:731','default','Maximum link text length','none');
INSERT INTO `esp_locales_source` VALUES ('1351',' modules/filter/filter.module:734','default','URLs longer than this number of characters will be truncated to prevent long strings that break formatting. The link itself will be retained; just the text portion of the link will be truncated.','none'),('1352',' modules/filter/filter.module:615,726','default','URL filter','6.19'),('1353',' modules/filter/filter.module:205','default','Acronym','none'),('1354',' modules/filter/filter.module:204','default','Abbreviation','none'),('1355',' modules/filter/filter.module:212','default','Inserted','none'),('1356',' modules/filter/filter.admin.inc:33','default','All roles may use default format','none'),('1357',' modules/filter/filter.admin.inc:33','default','No roles may use this format','none'),('1358',' modules/filter/filter.admin.inc:38','default','Set default format','none'),('1359',' modules/filter/filter.admin.inc:45','default','Default format updated.','none'),('1360',' modules/filter/filter.admin.inc:97','default','All roles for the default format must be enabled and cannot be changed.','none'),('1361',' modules/filter/filter.admin.inc:104','default','Specify a unique name for this filter format.','none'),('1362',' modules/filter/filter.admin.inc:111','default','Choose which roles may use this filter format. Note that roles with the \"administer filters\" permission can always use all the filter formats.','none'),('1363',' modules/filter/filter.admin.inc:131','default','Choose the filters that will be used in this filter format.','none'),('1364',' modules/filter/filter.admin.inc:149','default','No guidelines available.','none'),('1365',' modules/filter/filter.admin.inc:151','default','These are the guidelines that users will see for posting in this input format. They are automatically generated from the filter settings.','none'),('1366',' modules/filter/filter.admin.inc:168','default','Filter format names need to be unique. A format named %name already exists.','none'),('1367',' modules/filter/filter.admin.inc:187','default','Added input format %format.','none');
INSERT INTO `esp_locales_source` VALUES ('1368',' modules/filter/filter.admin.inc:190','default','The input format settings have been updated.','none'),('1369',' modules/filter/filter.admin.inc:252','default','Are you sure you want to delete the input format %format?','none'),('1370',' modules/filter/filter.admin.inc:252','default','If you have any content left in this input format, it will be switched to the default input format. This action cannot be undone.','none'),('1371',' modules/filter/filter.admin.inc:255','default','The default format cannot be deleted.','none'),('1372',' modules/filter/filter.admin.inc:278','default','Deleted input format %format.','none'),('1373',' modules/filter/filter.admin.inc:289','default','Configure %format','none'),('1374',' modules/filter/filter.admin.inc:312','default','No settings are available.','none'),('1375',' modules/filter/filter.admin.inc:330','default','Rearrange %format','none'),('1376',' modules/filter/filter.admin.inc:391','default','The filter ordering has been saved.','none'),('1377',' modules/filter/filter.module:23','default','The filter module allows administrators to configure text input formats for use on your site. An input format defines the HTML tags, codes, and other input allowed in both content and comments, and is a key feature in guarding against potentially damaging input from malicious users. Two input formats included by default are <em>Filtered HTML</em> (which allows only an administrator-approved subset of HTML tags) and <em>Full HTML</em> (which allows the full set of HTML tags). Additional input formats may be created by an administrator.','none'),('1378',' modules/filter/filter.module:24','default','Each input format uses filters to manipulate text, and most input formats apply several different filters to text in a specific order. Each filter is designed for a specific purpose, and generally either adds, removes or transforms elements within user-entered text before it is displayed. A filter does not change the actual content of a post, but instead, modifies it temporarily before it is displayed. A filter may remove unapproved HTML tags, for instance, while another automatically adds HTML to make links referenced in text clickable.','none');
INSERT INTO `esp_locales_source` VALUES ('1379',' modules/filter/filter.module:25','default','Users can choose between the available input formats when creating or editing content. Administrators can configure which input formats are available to which user roles, as well as choose a default input format.','none'),('1380',' modules/filter/filter.module:26','default','For more information, see the online handbook entry for <a href=\"@filter\">Filter module</a>.','none'),('1381',' modules/filter/filter.module:29','default','<em>Input formats</em> define a way of processing user-supplied text in Drupal. Each input format uses filters to manipulate text, and most input formats apply several different filters to text, in a specific order. Each filter is designed to accomplish a specific purpose, and generally either removes elements from or adds elements to text before it is displayed. Users can choose between the available input formats when submitting content.','none'),('1382',' modules/filter/filter.module:30','default','Use the list below to configure which input formats are available to which roles, as well as choose a default input format (used for imported content, for example). The default format is always available to users. All input formats are available to users in a role with the \"administer filters\" permission.','none'),('1383',' modules/filter/filter.module:33','default','Every <em>filter</em> performs one particular change on the user input, for example stripping out malicious HTML or making URLs clickable. Choose which filters you want to apply to text in this input format. If you notice some filters are causing conflicts in the output, you can <a href=\"@rearrange\">rearrange them</a>.','none'),('1384',' modules/filter/filter.module:35','default','If you cannot find the settings for a certain filter, make sure you have enabled it on the <a href=\"@url\">view tab</a> first.','none'),('1385',' modules/filter/filter.module:37','default','Because of the flexible filtering system, you might encounter a situation where one filter prevents another from doing its job. For example: a word in an URL gets converted into a glossary term, before the URL can be converted to a clickable link. When this happens, rearrange the order of the filters.','none');
INSERT INTO `esp_locales_source` VALUES ('1386',' modules/filter/filter.module:38','default','Filters are executed from top-to-bottom. To change the order of the filters, modify the values in the <em>Weight</em> column or grab a drag-and-drop handle under the <em>Name</em> column and drag filters to new locations in the list. (Grab a handle by clicking and holding the mouse while hovering over a handle icon.) Remember that your changes will not be saved until you click the <em>Save configuration</em> button at the bottom of the page.','none'),('1387',' modules/filter/filter.module:180,182','default','Allowed HTML tags: @tags','6.19'),('1388',' modules/filter/filter.module:187','default','\n<p>This site allows HTML content. While learning all of HTML may feel intimidating, learning how to use a very small number of the most basic HTML \"tags\" is very easy. This table provides examples for each tag that is enabled on this site.</p>\n<p>For more information see W3C\'s <a href=\"http://www.w3.org/TR/html/\">HTML Specifications</a> or use your favorite search engine to find other sites that explain HTML.</p>','none'),('1389',' modules/filter/filter.module:191','default','Anchors are used to make links to other pages.','none'),('1390',' modules/filter/filter.module:192','default','By default line break tags are automatically added, so use this tag to add additional ones. Use of this tag is different because it is not used with an open/close pair like all the others. Use the extra \" /\" inside the tag to maintain XHTML 1.0 compatibility','none'),('1391',' modules/filter/filter.module:192','default','Text with <br />line break','none'),('1392',' modules/filter/filter.module:193','default','By default paragraph tags are automatically added, so use this tag to add additional ones.','none'),('1393',' modules/filter/filter.module:193','default','Paragraph one.','none'),('1394',' modules/filter/filter.module:193','default','Paragraph two.','none'),('1395',' modules/filter/filter.module:194','default','Strong','none'),('1396',' modules/filter/filter.module:195','default','Emphasized','none');
INSERT INTO `esp_locales_source` VALUES ('1397',' modules/filter/filter.module:196','default','Cited','none'),('1398',' modules/filter/filter.module:197','default','Coded text used to show programming source code','none'),('1399',' modules/filter/filter.module:197','default','Coded','none'),('1400',' modules/filter/filter.module:198','default','Bolded','none'),('1401',' modules/filter/filter.module:200','default','Italicized','none'),('1402',' modules/filter/filter.module:201','default','Superscripted','none'),('1403',' modules/filter/filter.module:201','default','<sup>Super</sup>scripted','none'),('1404',' modules/filter/filter.module:202','default','Subscripted','none'),('1405',' modules/filter/filter.module:202','default','<sub>Sub</sub>scripted','none'),('1406',' modules/filter/filter.module:204','default','<abbr title=\"Abbreviation\">Abbrev.</abbr>','none'),('1407',' modules/filter/filter.module:205','default','<acronym title=\"Three-Letter Acronym\">TLA</acronym>','none'),('1408',' modules/filter/filter.module:206','default','Block quoted','none'),('1409',' modules/filter/filter.module:207','default','Quoted inline','none'),('1410',' modules/filter/filter.module:209','default','Table header','none'),('1411',' modules/filter/filter.module:209','default','Table cell','none'),('1412',' modules/filter/filter.module:214','default','Ordered list - use the &lt;li&gt; to begin each list item','none'),('1413',' modules/filter/filter.module:214,215','default','First item','none'),('1414',' modules/filter/filter.module:214,215','default','Second item','none'),('1415',' modules/filter/filter.module:215','default','Unordered list - use the &lt;li&gt; to begin each list item','none'),('1416',' modules/filter/filter.module:218','default','Definition lists are similar to other HTML lists. &lt;dl&gt; begins the definition list, &lt;dt&gt; begins the definition term and &lt;dd&gt; begins the definition description.','none'),('1417',' modules/filter/filter.module:218','default'