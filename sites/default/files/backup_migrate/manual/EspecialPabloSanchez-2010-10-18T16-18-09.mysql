INSERT INTO `esp_locales_source` VALUES ('485',' modules/system/system.install:290','default','HTTP request status','none'),('486',' modules/system/system.install:291','default','Fails','none'),('487',' modules/system/system.install:293','default','Your system or network configuration does not allow Drupal to access web pages, resulting in reduced functionality. This could be due to your webserver configuration or PHP settings, and should be resolved in order to download information about available updates, fetch aggregator feeds, sign in via OpenID, or use other network-dependent services.','none'),('488',' modules/system/system.admin.inc:167','default','Choose which theme the administration pages should display in. If you choose \"System default\" the administration pages will use the same theme as the rest of the site.','none'),('489',' modules/system/system.admin.inc:295','default','Please note that the <a href=\"!admin_theme_page\">administration theme</a> is still set to the %admin_theme theme; consequently, the theme on this page remains unchanged. All non-administrative sections of the site, however, will show the selected %selected_theme theme by default.','none'),('490',' modules/system/system.admin.inc:388','default','User pictures in posts','6.19'),('491',' modules/system/system.admin.inc:389','default','User pictures in comments','6.19'),('492',' modules/system/system.admin.inc:391','default','Shortcut icon','6.19'),('493',' modules/system/system.admin.inc:434','default','Display post information on','none'),('494',' modules/system/system.admin.inc:435','default','Enable or disable the <em>submitted by Username on date</em> text when displaying posts of the following type.','none'),('495',' modules/system/system.admin.inc:454','default','Logo image settings','6.19'),('496',' modules/system/system.admin.inc:455','default','If toggled on, the following logo will be displayed.','6.19'),('497',' modules/system/system.admin.inc:460','default','Use the default logo','6.19'),('498',' modules/system/system.admin.inc:463','default','Check here if you want the theme to use the logo supplied with it.','6.19');
INSERT INTO `esp_locales_source` VALUES ('499',' modules/system/system.admin.inc:469','default','The path to the file you would like to use as your logo file instead of the default logo.','6.19'),('500',' modules/system/system.admin.inc:473','default','Upload logo image','6.19'),('501',' modules/system/system.admin.inc:482','default','Shortcut icon settings','6.19'),('502',' modules/system/system.admin.inc:483','default','Your shortcut icon, or \'favicon\', is displayed in the address bar and bookmarks of most browsers.','6.19'),('503',' modules/system/system.admin.inc:487','default','Use the default shortcut icon.','6.19'),('504',' modules/system/system.admin.inc:489','default','Check here if you want the theme to use the default shortcut icon.','6.19'),('505',' modules/system/system.admin.inc:495','default','The path to the image file you would like to use as your custom shortcut icon.','6.19'),('506',' modules/system/system.admin.inc:500','default','Upload icon image','6.19'),('507',' modules/system/system.admin.inc:501','default','If you don\'t have direct file access to the server, use this field to upload your shortcut icon.','6.19'),('508',' modules/system/system.admin.inc:689','default','@module (<span class=\"admin-missing\">missing</span>)','6.19'),('509',' modules/system/system.admin.inc:1082','default','The following modules will be completely uninstalled from your site, and <em>all data from these modules will be lost</em>!','none'),('510',' modules/system/system.admin.inc:1085','default','Confirm uninstall','none'),('511',' modules/system/system.admin.inc:1087','default','Would you like to continue with uninstalling the above?','none'),('512',' modules/system/system.admin.inc:1100','default','No modules selected.','none'),('513',' modules/system/system.admin.inc:1117','default','The selected modules have been uninstalled.','none'),('514',' modules/system/system.admin.inc:1138','default','The name of this website.','none'),('515',' modules/system/system.admin.inc:1152','default','Your site\'s motto, tag line, or catchphrase (often displayed alongside the title of the site).','none');
INSERT INTO `esp_locales_source` VALUES ('516',' modules/system/system.admin.inc:1158','default','Your site\'s mission or focus statement (often prominently displayed on the front page).','none'),('517',' modules/system/system.admin.inc:1203','default','The path \'@path\' is either invalid or you do not have access to it.','none'),('518',' modules/system/system.admin.inc:1217','default','Default 403 (access denied) page','none'),('519',' modules/system/system.admin.inc:1220','default','This page is displayed when the requested document is denied to the current user. If unsure, specify nothing.','none'),('520',' modules/system/system.admin.inc:1226','default','Default 404 (not found) page','none'),('521',' modules/system/system.admin.inc:1235','default','Write errors to the log','none'),('522',' modules/system/system.admin.inc:1235','default','Write errors to the log and to the screen','none'),('523',' modules/system/system.admin.inc:1236','default','Specify where Drupal, PHP and SQL errors are logged. While it is recommended that a site running in a production environment write errors to the log only, in a development or testing environment it may be helpful to write errors both to the log and to the screen.','none'),('524',' modules/system/system.admin.inc:1262','default','The normal cache mode is suitable for most sites and does not cause any side effects. The aggressive cache mode causes Drupal to skip the loading (boot) and unloading (exit) of enabled modules when serving a cached page. This results in an additional performance boost but can cause unwanted side effects.','none'),('525',' modules/system/system.admin.inc:1268','default','<strong class=\"error\">The following enabled modules are incompatible with aggressive mode caching and will not function properly: %modules</strong>','none'),('526',' modules/system/system.admin.inc:1271','default','<strong class=\"ok\">Currently, all enabled modules are compatible with the aggressive caching policy.</strong> Please note, if you use aggressive caching and enable new modules, you will need to check this page again to ensure compatibility.','none');
INSERT INTO `esp_locales_source` VALUES ('527',' modules/system/system.admin.inc:1275','default','Page cache','none'),('528',' modules/system/system.admin.inc:1276','default','Enabling the page cache will offer a significant performance boost. Drupal can store and send compressed cached pages requested by <em>anonymous</em> users. By caching a web page, Drupal does not have to construct the page each time it is viewed.','none'),('529',' modules/system/system.admin.inc:1281','default','Caching mode','none'),('530',' modules/system/system.admin.inc:1283','default','Normal (recommended for production sites, no side effects)','none'),('531',' modules/system/system.admin.inc:1283','default','Aggressive (experts only, possible side effects)','none'),('532',' modules/system/system.admin.inc:1294','default','On high-traffic sites, it may be necessary to enforce a minimum cache lifetime. The minimum cache lifetime is the minimum amount of time that will elapse before the cache is emptied and recreated, and is applied to both page and block caches. A larger minimum cache lifetime offers better performance, but users will not see new content for a longer period of time.','none'),('533',' modules/system/system.admin.inc:1298','default','Page compression','none'),('534',' modules/system/system.admin.inc:1301','default','By default, Drupal compresses the pages it caches in order to save bandwidth and improve download times. This option should be disabled when using a webserver that performs compression.','none'),('535',' modules/system/system.admin.inc:1307','default','Enabling the block cache can offer a performance increase for all users by preventing blocks from being reconstructed on each page load. If the page cache is also enabled, performance increases from enabling the block cache will mainly benefit authenticated users.','none'),('536',' modules/system/system.admin.inc:1314','default','Enabled (recommended)','none'),('537',' modules/system/system.admin.inc:1316','default','Note that block caching is inactive when modules defining content access restrictions are enabled.','none');
INSERT INTO `esp_locales_source` VALUES ('538',' modules/system/system.admin.inc:1321','default','Bandwidth optimizations','none'),('539',' modules/system/system.admin.inc:1322','default','<p>Drupal can automatically optimize external resources like CSS and JavaScript, which can reduce both the size and number of requests made to your website. CSS files can be aggregated and compressed into a single file, while JavaScript files are aggregated (but not compressed). These optional optimizations may reduce server load, bandwidth requirements, and page loading times.</p><p>These options are disabled if you have not set up your files directory, or if your download method is set to private.</p>','none'),('540',' modules/system/system.admin.inc:1329','default','Optimize CSS files','none'),('541',' modules/system/system.admin.inc:1333','default','This option can interfere with theme development and should only be enabled in a production environment.','none'),('542',' modules/system/system.admin.inc:1341','default','This option can interfere with module development and should only be enabled in a production environment.','none'),('543',' modules/system/system.admin.inc:1346,1352','default','Clear cached data','none'),('544',' modules/system/system.admin.inc:1347','default','Caching data improves performance, but may cause problems while troubleshooting new modules, themes, or translations, if outdated information has been cached. To refresh all cached data on your site, click the button below. <em>Warning: high-traffic sites will experience performance slowdowns while cached data is rebuilt.</em>','none'),('545',' modules/system/system.admin.inc:1369','default','Caches cleared.','none'),('546',' modules/system/system.admin.inc:1385','default','A file system path where the files will be stored. This directory must exist and be writable by Drupal. If the download method is set to public, this directory must be relative to the Drupal installation directory and be accessible over the web. If the download method is set to private, this directory should not be accessible over the web. Changing this location will modify all download paths and may cause unexpected problems on an existing site.','none');
INSERT INTO `esp_locales_source` VALUES ('547',' modules/system/system.admin.inc:1394','default','A file system path where uploaded files will be stored during previews.','none'),('548',' modules/system/system.admin.inc:1402','default','Public - files are available using HTTP directly.','none'),('549',' modules/system/system.admin.inc:1402','default','Private - files are transferred by Drupal.','none'),('550',' modules/system/system.admin.inc:1403','default','Choose the <em>Public download</em> method unless you wish to enforce fine-grained access controls over file downloads. Changing the download method will modify all download paths and may cause unexpected problems on an existing site.','none'),('551',' modules/system/system.admin.inc:1420','default','Select an image processing toolkit','none'),('552',' modules/system/system.admin.inc:1444','default','Number of items in each feed','none'),('553',' modules/system/system.admin.inc:1447','default','Default number of items to include in each feed.','none'),('554',' modules/system/system.admin.inc:1451','default','Feed content','none'),('555',' modules/system/system.admin.inc:1453','default','Titles plus teaser','none'),('556',' modules/system/system.admin.inc:1454','default','Global setting for the default display of content items in each feed.','none'),('557',' modules/system/system.admin.inc:1514','default','User-configurable time zones','none'),('558',' modules/system/system.admin.inc:1517','default','When enabled, users can set their own time zone and dates will be displayed accordingly.','none'),('559',' modules/system/system.admin.inc:1525','default','The first day of the week for calendar views.','none'),('560',' modules/system/system.admin.inc:1530','default','Formatting','none'),('561',' modules/system/system.admin.inc:1538','default','Short date format','none'),('562',' modules/system/system.admin.inc:1542','default','The short format of date display.','none'),('563',' modules/system/system.admin.inc:1550','default','Custom short date format','none'),('564',' modules/system/system.admin.inc:1553','default','A user-defined short date format. See the <a href=\"@url\">PHP manual</a> for available options. This format is currently set to display as <span>%date</span>.','none');
INSERT INTO `esp_locales_source` VALUES ('565',' modules/system/system.admin.inc:1561','default','Medium date format','none'),('566',' modules/system/system.admin.inc:1565','default','The medium sized date display.','none'),('567',' modules/system/system.admin.inc:1573','default','Custom medium date format','none'),('568',' modules/system/system.admin.inc:1576','default','A user-defined medium date format. See the <a href=\"@url\">PHP manual</a> for available options. This format is currently set to display as <span>%date</span>.','none'),('569',' modules/system/system.admin.inc:1584','default','Long date format','none'),('570',' modules/system/system.admin.inc:1588','default','Longer date format used for detailed display.','none'),('571',' modules/system/system.admin.inc:1596','default','Custom long date format','none'),('572',' modules/system/system.admin.inc:1599','default','A user-defined long date format. See the <a href=\"@url\">PHP manual</a> for available options. This format is currently set to display as <span>%date</span>.','none'),('573',' modules/system/system.admin.inc:1678','default','Before enabling clean URLs, you must perform a test to determine if your server is properly configured. If you are able to see this page again after clicking the \"Run the clean URL test\" link, the test has succeeded and the radio buttons above will be available. If instead you are directed to a \"Page not found\" error, you will need to change the configuration of your server. The <a href=\"@handbook\">handbook page on Clean URLs</a> has additional troubleshooting information.','none'),('574',' modules/system/system.admin.inc:1682','default','<a href=\"@clean_url\">Run the clean url test</a>.','none'),('575',' modules/system/system.admin.inc:1723','default','Cron ran successfully.','6.19'),('576',' modules/system/system.admin.inc:1726','default','Cron run failed.','none'),('577',' modules/system/system.admin.inc:1773','default','Command counters','none'),('578',' modules/system/system.admin.inc:1775','default','The number of <code>SELECT</code>-statements.','none');
INSERT INTO `esp_locales_source` VALUES ('579',' modules/system/system.admin.inc:1776','default','The number of <code>INSERT</code>-statements.','none'),('580',' modules/system/system.admin.inc:1777','default','The number of <code>UPDATE</code>-statements.','none'),('581',' modules/system/system.admin.inc:1778','default','The number of <code>DELETE</code>-statements.','none'),('582',' modules/system/system.admin.inc:1779','default','The number of table locks.','none'),('583',' modules/system/system.admin.inc:1780','default','The number of table unlocks.','none'),('584',' modules/system/system.admin.inc:1783','default','Query performance','none'),('585',' modules/system/system.admin.inc:1785','default','The number of joins without an index; should be zero.','none'),('586',' modules/system/system.admin.inc:1786','default','The number of joins without keys that check for key usage after each row; should be zero.','none'),('587',' modules/system/system.admin.inc:1787','default','The number of sorts done without using an index; should be zero.','none'),('588',' modules/system/system.admin.inc:1788','default','The number of times a lock could be acquired immediately.','none'),('589',' modules/system/system.admin.inc:1789','default','The number of times the server had to wait for a lock.','none'),('590',' modules/system/system.admin.inc:1792','default','Query cache information','none'),('591',' modules/system/system.admin.inc:1793','default','The MySQL query cache can improve performance of your site by storing the result of queries. Then, if an identical query is received later, the MySQL server retrieves the result from the query cache rather than parsing and executing the statement again.','none'),('592',' modules/system/system.admin.inc:1795','default','The number of queries in the query cache.','none'),('593',' modules/system/system.admin.inc:1796','default','The number of times MySQL found previous results in the cache.','none'),('594',' modules/system/system.admin.inc:1797','default','The number of times MySQL added a query to the cache (misses).','none');
INSERT INTO `esp_locales_source` VALUES ('595',' modules/system/system.admin.inc:1798','default','The number of times MySQL had to remove queries from the cache because it ran out of memory. Ideally should be zero.','none'),('596',' modules/system/system.admin.inc:2130','default','No modules are available to uninstall.','none'),('597',' modules/system/system.admin.inc:2192','default','This theme requires PHP version @php_required and is incompatible with PHP version !php_version.','none'),('598',' modules/system/system.admin.inc:836','default','You must enable the @dependencies module to install @module.','6.19'),('599',' modules/system/system.admin.inc:836','default','You must enable the @dependencies modules to install @module.','none'),('600',' modules/system/system.module:52','default','The system module is at the foundation of your Drupal website, and provides basic but extensible functionality for use by other modules and themes. Some integral elements of Drupal are contained in and managed by the system module, including caching, enabling or disabling of modules and themes, preparing and displaying the administrative page, and configuring fundamental site settings. A number of key system maintenance operations are also part of the system module.','none'),('601',' modules/system/system.module:53','default','The system module provides:','none'),('602',' modules/system/system.module:54','default','support for enabling and disabling <a href=\"@modules\">modules</a>. Drupal comes packaged with a number of core modules; each module provides a discrete set of features and may be enabled depending on the needs of your site. A wide array of additional modules contributed by members of the Drupal community are available for download at the <a href=\"@drupal-modules\">Drupal.org module page</a>.','none'),('603',' modules/system/system.module:55','default','support for enabling and disabling <a href=\"@themes\">themes</a>, which determine the design and presentation of your site. Drupal comes packaged with several core themes and additional contributed themes are available at the <a href=\"@drupal-themes\">Drupal.org theme page</a>.','none');
INSERT INTO `esp_locales_source` VALUES ('604',' modules/system/system.module:56','default','a robust <a href=\"@cache-settings\">caching system</a> that allows the efficient re-use of previously-constructed web pages and web page components. Drupal stores the pages requested by anonymous users in a compressed format; depending on your site configuration and the amount of your web traffic tied to anonymous visitors, Drupal\'s caching system may significantly increase the speed of your site.','none'),('605',' modules/system/system.module:57','default','a set of routine administrative operations that rely on a correctly-configured <a href=\"@cron\">cron maintenance task</a> to run automatically. A number of other modules, including the feed aggregator, ping module and search also rely on <a href=\"@cron\">cron maintenance tasks</a>. For more information, see the online handbook entry for <a href=\"@handbook\">configuring cron jobs</a>.','none'),('606',' modules/system/system.module:58','default','basic configuration options for your site, including <a href=\"@date-settings\">date and time settings</a>, <a href=\"@file-system\">file system settings</a>, <a href=\"@clean-url\">clean URL support</a>, <a href=\"@site-info\">site name and other information</a>, and a <a href=\"@site-maintenance\">site maintenance</a> function for taking your site temporarily off-line.','none'),('607',' modules/system/system.module:59','default','For more information, see the online handbook entry for <a href=\"@system\">System module</a>.','none'),('608',' modules/system/system.module:62','default','Welcome to the administration section. Here you may control how your site functions.','6.19'),('609',' modules/system/system.module:64','default','This page shows you all available administration tasks for each module.','none'),('610',' modules/system/system.module:66','default','Select which themes are available to your users and specify the default theme. To configure site-wide display settings, click the \"configure\" task above. Alternatively, to override these settings in a specific theme, click the \"configure\" link for that theme. Note that different themes may have different regions available for displaying content; for consistency in presentation, you may wish to enable only one theme.','6.19');
INSERT INTO `esp_locales_source` VALUES ('611',' modules/system/system.module:67','default','To change the appearance of your site, a number of <a href=\"@themes\">contributed themes</a> are available.','6.19'),('612',' modules/system/system.module:72','default','These options control the display settings for the <code>%template</code> theme. When your site is displayed using this theme, these settings will be used. By clicking \"Reset to defaults,\" you can choose to use the <a href=\"@global\">global settings</a> for this theme.','6.19'),('613',' modules/system/system.module:74','default','These options control the default display settings for your entire site, across all themes. Unless they have been overridden by a specific theme, these settings will be used.','none'),('614',' modules/system/system.module:76','default','Modules are plugins that extend Drupal\'s core functionality. Enable modules by selecting the <em>Enabled</em> checkboxes below and clicking the <em>Save configuration</em> button. Once a module is enabled, new <a href=\"@permissions\">permissions</a> may be available. To reduce server load, modules with their <em>Throttle</em> checkbox selected are temporarily disabled when your site becomes extremely busy. (Note that the <em>Throttle</em> checkbox is only available if the Throttle module is enabled.)','6.19'),('615',' modules/system/system.module:78','default','The auto-throttle functionality must be enabled on the <a href=\"@throttle\">throttle configuration page</a> after having enabled the throttle module.','none'),('616',' modules/system/system.module:81','default','It is important that <a href=\"@update-php\">update.php</a> is run every time a module is updated to a newer version.','6.19'),('617',' modules/system/system.module:82','default','You can find all administration tasks belonging to a particular module on the <a href=\"@by-module\">administration by module page</a>.','6.19'),('618',' modules/system/system.module:83','default','To extend the functionality of your site, a number of <a href=\"@modules\">contributed modules</a> are available.','6.19');
INSERT INTO `esp_locales_source` VALUES ('619',' modules/system/system.module:87','default','The uninstall process removes all data related to a module. To uninstall a module, you must first disable it. Not all modules support this feature.','none'),('620',' modules/system/system.module:90','default','The <em>Powered by Drupal</em> block is an optional link to the home page of the Drupal project. While there is absolutely no requirement that sites feature this link, it may be used to show support for Drupal.','none'),('621',' modules/system/system.module:104','default','Here you can find a short overview of your site\'s parameters as well as any problems detected with your installation. It may be useful to copy and paste this information into support requests filed on drupal.org\'s support forums and project issue queues.','6.19'),('622',' modules/system/system.module:553','default','Selecting a different theme will change the look and feel of the site.','none'),('623',' modules/system/system.module:568','default','Select your current local time. Dates and times throughout this site will be displayed using this time zone.','none'),('624',' modules/system/system.module:585','default','Powered by Drupal','6.19'),('625',' modules/system/system.module:595','default','Badge color','none'),('626',' modules/system/system.module:601','default','Badge size','none'),('627',' modules/system/system.module:1655','default','The message that should be sent. You may include the following variables: %site_name, %username, %node_url, %node_type, %title, %teaser, %body. Not all variables will be available in all contexts.','none'),('628',' modules/system/system.module:1801','default','The message to be displayed to the current user. You may include the following variables: %site_name, %username, %node_url, %node_type, %title, %teaser, %body. Not all variables will be available in all contexts.','none'),('629',' modules/system/system.module:1931','default','Powered by Drupal, an open source content management system','6.19'),('630',' modules/system/system.module:1281','default','Could not delete temporary file \"%path\" during garbage collection','none');
