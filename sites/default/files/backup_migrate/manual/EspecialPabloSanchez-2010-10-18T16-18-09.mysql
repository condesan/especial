INSERT INTO `esp_locales_source` VALUES ('2005',' modules/user/user.pages.inc:122','default','This login can be used only once.','none'),('2006',' modules/user/user.pages.inc:129','default','You have tried to use a one-time login link which has either been used or is no longer valid. Please request a new one using the form below.','none'),('2007',' modules/user/user.module:1468','default','Provide a password for the new account in both fields.','none'),('2008',' modules/user/user.module:1655','default','Account details for !username at !site','none'),('2009',' modules/user/user.module:1657','default','!username,\n\nThank you for registering at !site. You may now log in to !login_uri using the following username and password:\n\nusername: !username\npassword: !password\n\nYou may also log in by clicking on this link or copying and pasting it in your browser:\n\n!login_url\n\nThis is a one-time login, so it can be used only once.\n\nAfter logging in, you will be redirected to !edit_uri so you can change your password.\n\n\n--  !site team','none'),('2010',' modules/user/user.admin.inc:241','default','User e-mail settings','none'),('2011',' modules/user/user.module:692,751','default','Who\'s new','6.19'),('2012',' modules/user/user.module:1877','default','Are you sure you want to delete these users?','none'),('2013',' modules/user/user.module:1887','default','The users have been deleted.','none'),('2014',' modules/user/user.admin.inc:890','default','allow','none'),('2015',' modules/user/user.admin.inc:748,885','default','Rule type','none'),('2016',' modules/user/user.admin.inc:754,885','default','Mask','none'),('2017',' modules/user/user.admin.inc:866','default','Are you sure you want to delete the @type rule for %rule?','none'),('2018',' modules/user/user.admin.inc:876','default','The access rule has been deleted.','none'),('2019',' modules/user/user.admin.inc:233','default','User registration settings','none'),('2020',' modules/user/user.admin.inc:234','default','Visi