INSERT INTO `esp_locales_source` VALUES ('1145',' modules/block/block.admin.inc:179','default','\'%name\' block','6.19'),('1146',' modules/block/block.admin.inc:185','default','User specific visibility settings','6.19'),('1147',' modules/block/block.admin.inc:190','default','Custom visibility settings','6.19'),('1148',' modules/block/block.admin.inc:192','default','Users cannot control whether or not they see this block.','6.19'),('1149',' modules/block/block.admin.inc:193','default','Show this block by default, but let individual users hide it.','6.19'),('1150',' modules/block/block.admin.inc:194','default','Hide this block by default but let individual users show it.','6.19'),('1151',' modules/block/block.admin.inc:196','default','Allow individual users to customize the visibility of this block in their account settings.','6.19'),('1152',' modules/block/block.admin.inc:218','default','Show block for specific roles','6.19'),('1153',' modules/block/block.admin.inc:221','default','Show this block only for the selected role(s). If you select no roles, the block will be visible to all users.','6.19'),('1154',' modules/block/block.admin.inc:226','default','Page specific visibility settings','6.19'),('1155',' modules/block/block.admin.inc:242','default','If the PHP-mode is chosen, enter PHP code between %php. Note that executing incorrect PHP-code can break your Drupal site.','6.19'),('1156',' modules/block/block.admin.inc:246','default','Show block on specific pages','6.19'),('1157',' modules/block/block.admin.inc:260','default','Save block','6.19'),('1158',' modules/block/block.admin.inc:269,298','default','Please ensure that each block description is unique.','none'),('1159',' modules/block/block.admin.inc:282','default','The block configuration has been saved.','none'),('1160',' modules/block/block.admin.inc:319','default','The block has been created.','none'),('1161',' modules/block/block.module:318','default','A brief description of your block. Used on the <a href=