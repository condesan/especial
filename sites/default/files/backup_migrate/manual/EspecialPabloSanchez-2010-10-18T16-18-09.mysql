INSERT INTO `esp_locales_source` VALUES ('1257',' modules/comment/comment.module:2010','default','After saving an updated comment','none'),('1258',' modules/comment/comment.module:2013','default','After deleting a comment','none'),('1259',' modules/comment/comment.module:2016','default','When a comment is being viewed by an authenticated user','none'),('1260',' modules/comment/comment.module:2065,2105','default','Unpublished comment %subject.','none'),('1261',' modules/comment/comment.module:2077','default','The comment will be unpublished if it contains any of the character sequences above. Use a comma-separated list of character sequences. Example: funny, bungee jumping, \"Company, Inc.\". Character sequences are case-sensitive.','none'),('1262',' modules/comment/comment.module:1051','default','Post new comment','none'),('1263',' modules/comment/comment.module:513','default','Default display order','6.19'),('1264',' modules/comment/comment.module:516','default','The default sorting for new users and anonymous users while viewing comments. These users may change their view using the comment control panel. For registered users, this change is remembered as a persistent user preference.','6.19'),('1265',' modules/comment/comment.module:136','default','Below is a list of the comments posted to your site that need approval. To approve a comment, click on \'edit\' and then change its \'moderation status\' to Approved. Click on a subject to see the comment, the author\'s name to edit the author\'s user information, \'edit\' to modify the text, and \'delete\' to remove their submission.','none'),('1266',' modules/comment/comment.module:199','default','List and edit site comments and the comment moderation queue.','6.19'),('1267',' modules/comment/comment.module:220','default','Delete comment','6.19'),('1268',' modules/comment/comment.module:235','default','Reply to comment','none'),('1269',' modules/comment/comment.module:506','default','Default display mode','6.19'),('1270',' modules/comment/comment.module:509','default','The default view for comments. Expanded views display the body of the comment. Threaded views keep replies together.','6.19');
INSERT INTO `esp_locales_source` VALUES ('1271',' modules/comment/comment.module:520','default','Default comments per page','6.19'),('1272',' modules/comment/comment.module:523','default','Default number of comments for each page: more comments are distributed in several pages.','6.19'),('1273',' modules/comment/comment.module:527','default','Comment controls','6.19'),('1274',' modules/comment/comment.module:530','default','Display above the comments','6.19'),('1275',' modules/comment/comment.module:531','default','Display below the comments','6.19'),('1276',' modules/comment/comment.module:532','default','Display above and below the comments','6.19'),('1277',' modules/comment/comment.module:534','default','Position of the comment controls box. The comment controls let the user change the default display mode and display order of comments.','6.19'),('1278',' modules/comment/comment.module:551','default','Comment subject field','6.19'),('1279',' modules/comment/comment.module:554','default','Can users provide a unique subject for their comments?','6.19'),('1280',' modules/comment/comment.pages.inc:65,105','default','You are not authorized to post comments.','none'),('1281',' modules/comment/comment.pages.inc:78,87','default','The comment you are replying to does not exist.','none'),('1282',' modules/comment/comment.pages.inc:98','default','This discussion is closed: you can\'t post new comments.','none'),('1283',' modules/comment/comment.pages.inc:111','default','You are not authorized to view comments.','none'),('1284',' modules/comment/comment.module:771','default','Comment: added %subject.','none'),('1285',' modules/comment/comment.admin.inc:245','default','Are you sure you want to delete the comment %title?','none'),('1286',' modules/comment/comment.admin.inc:247','default','Any replies to this comment will be lost. This action cannot be undone.','none'),('1287',' modules/comment/comment.module:1074,1086','default','Publish the selected comments','none'),('1288',