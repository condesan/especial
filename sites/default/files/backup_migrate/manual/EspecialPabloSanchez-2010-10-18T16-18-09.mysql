INSERT INTO `esp_locales_source` VALUES ('1725',' modules/node/content_types.inc:331','default','The content type %name has been updated.','none'),('1726',' modules/node/content_types.inc:334','default','The content type %name has been added.','6.19'),('1727',' modules/node/content_types.inc:75','default','The human-readable name of this content type. This text will be displayed as part of the list on the <em>create content</em> page. It is recommended that this name begin with a capital letter and contain only letters, numbers, and <strong>spaces</strong>. This name must be unique.','6.19'),('1728',' modules/node/content_types.inc:86','default','The machine-readable name of this content type. This text will be used for constructing the URL of the <em>create content</em> page for this content type. This name must contain only lowercase letters, numbers, and underscores. Underscores will be converted into hyphens when constructing the URL of the <em>create content</em> page. This name must be unique.','6.19'),('1729',' modules/node/content_types.inc:98','default','The machine-readable name of this content type. This field cannot be modified for system-defined content types.','none'),('1730',' modules/node/content_types.inc:106','default','A brief description of this content type. This text will be displayed as part of the list on the <em>create content</em> page.','6.19'),('1731',' modules/node/content_types.inc:111','default','Submission form settings','6.19'),('1732',' modules/node/content_types.inc:125','default','This content type does not have a title field.','none'),('1733',' modules/node/content_types.inc:132','default','To omit the body field for this content type, remove any text and leave this field blank.','6.19'),('1734',' modules/node/content_types.inc:145','default','This text will be displayed at the top of the submission form for this content type. It is useful for helping or instructing your users.','6.19'),('1735',' modules/node/content_types.inc:14