INSERT INTO `esp_locales_source` VALUES ('1555',' modules/menu/menu.admin.inc:629','default','Source for the secondary links','none'),('1556',' modules/menu/menu.admin.inc:295','default','Menu items that are not enabled will not be listed in any menu.','none'),('1557',' modules/menu/menu.module:28','default','Enter the name for your new menu. Remember to enable the newly created block in the <a href=\"@blocks\">blocks administration page</a>.','none'),('1558',' modules/menu/menu.module:30','default','To rearrange menu items, grab a drag-and-drop handle under the <em>Menu item</em> column and drag the items (or group of items) to a new location in the list. (Grab a handle by clicking and holding the mouse while hovering over a handle icon.) Remember that your changes will not be saved until you click the <em>Save configuration</em> button at the bottom of the page.','none'),('1559',' modules/menu/menu.module:57','default','List menus','none'),('1560',' modules/menu/menu.module:80','default','Customize menu','none'),('1561',' modules/menu/menu.module:90','default','List items','none'),('1562',' modules/menu/menu.module:48','default','Menus','6.19'),('1563',' modules/menu/menu.module:49','default','Control your site\'s navigation menu, primary links and secondary links. as well as rename and reorganize menu items.','6.19'),('1564',' modules/menu/menu.admin.inc:584','default','Are you sure you want to reset the item %item to its default values?','none'),('1565',' modules/menu/menu.admin.inc:584','default','Any customizations will be lost. This action cannot be undone.','none'),('1566',' modules/menu/menu.admin.inc:593','default','The menu item was reset to its default settings.','none'),('1567',' modules/menu/menu.module:40','default','administer menu','none'),('1568',' modules/menu/menu.admin.inc:54','default','There are no menu items yet.','none'),('1569',' modules/menu/menu.admin.inc:281','default','The link text corresponding to this item that should appear in the m